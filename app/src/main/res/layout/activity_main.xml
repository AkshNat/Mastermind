        android:layout_height="wrap_content"
        android:id="@+id/editText9"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true"
        android:maxLines="1"
        android:maxLength="4"
        android:imeOptions="actionSearch"/>

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
